;; shadow-cljs configuration
{:deps true

 :builds
 {:app {:target :browser
        :output-dir "resources/public/js"
        :asset-path "/js"
        :modules {:main {:init-fn user.comm/main!}}
        :dev {:compiler-options {:optimizations :none}}
        :devtools {:after-load user.comm/main!
                   :preloads     [devtools.preload]}}}}
